@model List<Job>
<section id="hero" class="hero section dark-background">

    <img src="/iPortfolio-1.0.0/assets/img/hero-bg.jpg" alt="" data-aos="fade-in" class="">

    <div class="container" data-aos="fade-up" data-aos-delay="100">
        @foreach(var item in Model)
        {
        <h2>@item.Name</h2>
        <p>@item.Title<span class="typed" data-typed-items="@item.Description">Designer</span><span class="typed-cursor typed-cursor--blink" aria-hidden="true"></span><span class="typed-cursor typed-cursor--blink" aria-hidden="true"></span></p>
    
        }
        </div>
</section><!-- /Hero Section -->
